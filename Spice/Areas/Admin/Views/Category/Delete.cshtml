@model CategoryModel


<br />
<h2 class="text-info">Usuń Kategorie</h2>
<br />

<form method="POST" asp-action="Delete" autocomplete="off">
    <div class="border background-white">
        <div class="form-group row">
            <div class="col-2">
                <label asp-for="Name" class="col-form-label"></label>
            </div>
            <div class="col-5">
                <input asp-for="Name" class="form-control" readonly />
            </div>
        </div>
        <div class="row form-group">
            <div class="col-5 offset-2">
                <partial name="_DeleteAndBackToListButtonPartial" model="Model.Id" />
            </div>
        </div>

    </div>
</form>
